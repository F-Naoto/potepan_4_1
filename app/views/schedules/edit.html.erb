<%= form_with(model:@schedule, local:true) do |f|%>
<%= render 'error_messages', object: f.object %>
<%= f.text_field :title %>
<%= f.date_field :start_date %>
<%= f.date_field :finish_date %>
<%= f.check_box :full_day %>
<%= f.text_area :memo %>

<%= f.submit 'スケジュールの編集を完了する' %>
<% end %>
<%= link_to "スケジュールの一覧に戻る", schedules_path %>